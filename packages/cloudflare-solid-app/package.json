{
  "name": "cloudflare-solid-app",
  "scripts": {
    "dev": "solid-start dev",
    "build": "solid-start build && wrangler pages functions build ./functions --output-routes-path ./pages/_routes.json --output-config-path ./pages/config.json --outfile ./pages/_worker.js",
    "start": "solid-start start",
    "deploy": "wrangler pages publish --project-name cloudflare-solid-app ./dist/public"
  },
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@types/cookie": "^0.5.1",
    "autoprefixer": "^10.4.9",
    "postcss": "^8.4.16",
    "solid-js": "^1.6.0",
    "solid-start": "^0.2.0",
    "solid-start-cloudflare-pages": "^0.2.0",
    "solid-start-cloudflare-workers": "^0.2.0",
    "solid-start-node": "^0.2.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "wrangler": "^2.1.13"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@solidjs/meta": "^0.28.1",
    "@solidjs/router": "^0.5.0",
    "cookie": "^0.5.0",
    "undici": "^5.10.0"
  },
  "engines": {
    "node": ">=16"
  }
}
