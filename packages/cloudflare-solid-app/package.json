{
  "name": "cloudflare-solid-app",
  "scripts": {
    "dev": "solid-start dev",
    "build": "solid-start build && wrangler pages functions build ./functions --output-routes-path ./pages/_routes.json --output-config-path ./pages/config.json --outfile ./pages/_worker.js",
    "start": "solid-start start",
    "deploy": "wrangler pages publish --project-name cloudflare-solid-app ./dist/public"
  },
  "type": "module",
  "devDependencies": {
    "@cloudflare/workers-types": "4.20230321.0",
    "@types/cookie": "0.5.1",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.21",
    "solid-js": "1.6.16",
    "solid-start": "0.2.24",
    "solid-start-cloudflare-pages": "0.2.24",
    "solid-start-cloudflare-workers": "0.2.24",
    "solid-start-node": "0.2.24",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.3",
    "vite": "4.2.1",
    "wrangler": "2.13.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "0.3.0",
    "@solidjs/meta": "0.28.2",
    "@solidjs/router": "0.5.1",
    "cookie": "0.5.0",
    "undici": "5.21.0"
  },
  "engines": {
    "node": ">=16"
  }
}
